{
  "name": "PLACE",
  "version": "1.0.0",
  "description": "Platform and Coworking Experience",
  "author": "#versusvirus",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "dev-local-socket": "NODE_ENV=develop nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "build:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt build",
    "generate:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt generate",
    "heroku-postbuild": "npm ci --prefix ./server"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/toast": "^3.3.0",
    "lodash.debounce": "^4.0.8",
    "nuxt": "^2.0.0",
    "nuxt-clipboard2": "^0.2.1",
    "nuxt-material-design-icons": "^1.0.4",
    "socket.io-client": "^2.3.0",
    "uuid": "^7.0.3",
    "vue-clickaway": "^2.2.2",
    "vue-moveable": "^1.6.1",
    "vuex-persist": "^2.2.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/stylelint-module": "^3.1.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^2.6.0",
    "lint-staged": "^8.2.1",
    "node-sass": "^4.13.1",
    "prettier": "^1.16.4",
    "sass-loader": "^8.0.2",
    "stylelint": "^10.1.0"
  }
}
